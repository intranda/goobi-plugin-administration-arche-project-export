        <select label="Attribution 4.0 International (CC BY 4.0)" value="https://vocabs.acdh.oeaw.ac.at/archelicenses/cc-by-4-0" />
        <select label="Attribution-NoDerivatives 4.0 International (CC BY-ND 4.0)" value="https://vocabs.acdh.oeaw.ac.at/archelicenses/cc-by-nd-4-0" />
        <select label="Attribution-NonCommercial 4.0 International (CC BY-NC 4.0)" value="https://vocabs.acdh.oeaw.ac.at/archelicenses/cc-by-nc-4-0" />
        <select label="Attribution-ShareAlike 4.0 International (CC BY-SA 4.0)" value="https://vocabs.acdh.oeaw.ac.at/archelicenses/cc-by-sa-4-0" />
        <select label="Attribution-NonCommercial-NoDerivatives 4.0 International (CC BY-NC-ND 4.0)" value="https://vocabs.acdh.oeaw.ac.at/archelicenses/cc-by-nc-nd-4-0" />
        <select label="Attribution-NonCommercial-ShareAlike 4.0 International (CC BY-NC-SA 4.0)" value="https://vocabs.acdh.oeaw.ac.at/archelicenses/cc-by-nc-sa-4-0" />
    </property>


    <!-- viewer url -->
    <viewerUrl>https://viewer.acdh.oeaw.ac.at/viewer</viewerUrl>

    <!-- export folder-->
    <exportFolder>/tmp</exportFolder>
</config_plugin>